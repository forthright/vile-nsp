{
  "name": "@brentlintner/vile-nsp",
  "version": "0.1.0",
  "description": "A vile plugin for checking against the Node Security Project",
  "main": "lib/index.js",
  "scripts": {
    "compile": "babel -d lib -m common src",
    "compile-dev": "bin/compile-dev",
    "dev": "bin/dev",
    "test": "bin/test",
    "test-cov": "bin/test-cov",
    "update": "ncu -ua",
    "vile": "vile -cpd --nodecorations"
  },
  "keywords": [
    "vile",
    "security",
    "node",
    "project",
    "nsp"
  ],
  "dependencies": {
    "@brentlintner/vile": "~0.4.1",
    "bluebird": "~3.1.1",
    "lodash": "~3.10.1",
    "nsp": "~2.2.0"
  },
  "devDependencies": {
    "babel": "~5.6.14",
    "chai": "~3.4.1",
    "chai-as-promised": "^5.2.0",
    "coffee-script": "~1.10.0",
    "istanbul": "~0.4.1",
    "mimus": "~0.1.2",
    "mocha": "~2.3.4",
    "release-it": "^2.3.1",
    "sinon": "~1.17.2",
    "sinon-chai": "~2.8.0"
  }
}
